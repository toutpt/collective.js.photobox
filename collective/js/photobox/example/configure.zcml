<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:genericsetup="http://namespaces.zope.org/genericsetup"
    i18n_domain="collective.js.photobox">

  <browser:page
    name="example.collective.js.photobox"
    for="*"
    class=".photobox.Example"
    template="photobox.pt"
    permission="zope2.View"
    />
  <browser:resource
    name="collective.js.photobox.example.main.js"
    file="main.js"/>

</configure>
